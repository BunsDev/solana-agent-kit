{
  "name": "solana-agent-kit",
  "version": "2.0.0-beta.4",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "description": "connect any ai agents to solana protocols",
  "scripts": {
    "clean": "rm -rf dist .turbo node_modules",
    "build": "tsc && tsc -p tsconfig.cjs.json && node ../../scripts/rename-to-cjs.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sendaifun/solana-agent-kit.git"
  },
  "homepage": "https://github.com/sendaifun/solana-agent-kit/tree/v2/packages/core",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "solana",
    "agent",
    "ai",
    "solana agent kit",
    "sendai"
  ],
  "author": "sendaifun",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@9.15.3",
  "dependencies": {
    "@solana/web3.js": "^1.98.0",
    "ai": "^4.1.5",
    "bs58": "^6.0.0",
    "zod": "^3.24.1"
  }
}
